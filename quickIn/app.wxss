/* 用户头像 */

.userinfo {
  display: flex;
  flex-direction: column;
  /* align-items: center; */
}

.user-btn {
  border-radius: 50%;
  margin-top: 30rpx;
  line-height: 100rpx;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx auto;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.circle {
  border: 1rpx solid orange;
  color: orange;
  border-radius: 20rpx;
  padding: 0 5rpx;
}

/* tabbar组件样式全局化 */

.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100rpx;
  border-top: 1rpx solid #d7d7d7;
}

.tabbar_nav {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  height: 100%;
}

.tabbar_icon {
  width: 34rpx;
  height: 37rpx;
  margin-bottom: 10rpx;
}

.tabbar_nav:nth-child(3) image {
  width: 89rpx;
  height: 89rpx;
  margin-bottom: 0;
}

/* .tabbar_nav:nth-child(2) text{  
    position:relative; 
    top:-32rpx;
} */

/* 右边箭头图标 */

.rightImg {
  width: 44rpx;
  height: 44rpx;
  margin: auto;
}

/* 按钮 */

.btn {
  width: 80%;
  margin: 50rpx auto;
  line-height: 80rpx;
  background: #6b427e;
  border-radius: 40rpx;
  color: #fff;
  text-align: center;
}

/* 弹窗 */

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 90;
  background: rgba(0, 0, 0, 1);
  opacity: 0.3;
}



/* tab部分 */
.tab {
  width: 100%;
  height: 84rpx;
  white-space: nowrap;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
}

.tab-item {
  display: inline-block;
  text-align: center;
  font-size: 26rpx;
  color: #8f9193;
}

.active {
  color: #6b427e;
  border-bottom: 4rpx solid #6b427e;
}